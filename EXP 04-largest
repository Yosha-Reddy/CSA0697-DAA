#include<stdio.h>
int main()
{
	int a[10],i,n,largest;
	printf("enter the number:");
	scanf("%d",&n);
	printf("enter the elements:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	largest=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]>largest)
		{
			largest=a[i];
		}
	}
	printf("largest element is %d",largest);
}
